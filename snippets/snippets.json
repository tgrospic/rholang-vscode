{
	"Create a new name": {
		"prefix": "new",
		"body": [
			"new $1 in {$2}",
		],
		"description": "Create a new name"
  },
  "Receive process": {
		"prefix": "for",
		"body": [
			"for($1 <- $2) {$3}",
		],
		"description": "Create a new receive process"
  },
  "Send process": {
		"prefix": "send",
		"body": [
			"$1!($1)",
		],
		"description": "Create a new send process"
  },
  "Contract": {
		"prefix": "contract",
		"body": [
			"contract $1($2) = {$3}",
		],
		"description": "Create a new contract (receive process)"
  },
  // Built-in processes
  "Standard output": {
		"prefix": "out",
		"body": [
			"$1(`rho:io:stdout`)",
		],
		"description": "Standard output"
  },
  "Standard error": {
		"prefix": "err",
		"body": [
			"$1(`rho:io:stderr`)",
		],
		"description": "Standard error"
	},
}
